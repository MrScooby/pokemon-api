generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  username  String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Pokemon {
  id        String   @id @default(uuid())
  name      String
  height    Float
  weight    Float
  image     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
